<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd">
<hibernate-reverse-engineering>
  <schema-selection match-catalog="access_db"/>
  <table-filter match-name="user_role"/>
  <table-filter match-name="role_permission"/>
  <table-filter match-name="user"/>
  <table-filter match-name="client"/>
  <table-filter match-name="role"/>
  <table-filter match-name="permission"/>
</hibernate-reverse-engineering>
